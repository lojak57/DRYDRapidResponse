{
  "version": 2,
  "name": "dryad-app",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": ".svelte-kit/output",
  "installCommand": "npm install",
  "framework": "sveltekit",
  "regions": ["sfo1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 